<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="facilities" class="org.springframework.web.servlet.view.jasperreports.JasperReportsMultiFormatView">
        <property name="url" value="/WEB-INF/reports/facilities.jasper"/>
        <property name="reportDataKey" value="datasource"/>
    </bean>

</beans>