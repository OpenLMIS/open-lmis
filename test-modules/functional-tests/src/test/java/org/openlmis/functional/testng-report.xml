<suite name="eLMIS Funtional Test" verbose="10" allow-return-values="true">
  <test name="eLMIS functional test">
    <groups>
      <run>
        <include name="report"/>
      </run>
    </groups>
    <classes preserver-order="true">
      <class name="org.openlmis.extension.functional.reports.ReportPermission">
      </class>
      <class name="org.openlmis.extension.functional.reports.FacilityMailingListReport">
      </class>
      <class name="org.openlmis.extension.functional.reports.DistrictConsumptionComparison">
      </class>
      <class name="org.openlmis.extension.functional.reports.E2EViewReport">
      </class>
      <class name="org.openlmis.extension.functional.reports.ConsumptionAverageByProductReport">
      </class>
      <class name="org.openlmis.extension.functional.reports.NonReportingFacilitiesReport">
      </class>
      <class name="org.openlmis.extension.functional.reports.StockedOutReport">
      </class>
      <class name="org.openlmis.extension.functional.reports.StockImbalanceByFacilityReport">
      </class>
      <class name="org.openlmis.extension.functional.reports.SummaryReport">
      </class>
      <class name="org.openlmis.extension.functional.reports.SupplyStatusByFacilityReprot">
      </class>
      <class name="org.openlmis.extension.functional.reports.ReportingRateReport">
      </class>
      <class name="org.openlmis.extension.functional.reports.AdjustmentSummaryReport">
      </class>
    </classes>
  </test>
</suite>

